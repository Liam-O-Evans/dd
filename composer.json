{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/addtoany": "1.15.0",
        "drupal/admin_toolbar": "2.4.0",
        "drupal/adminimal_admin_toolbar": "1.11",
        "drupal/adminimal_theme": "1.6.0",
        "drupal/advanced_help": "^1.0@beta",
        "drupal/anchor_link": "2.5.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "5.0.0-beta3",
        "drupal/blocache": "1.1",
        "drupal/bootstrap_barrio": "5.1.3",
        "drupal/cas": "1.7.0",
        "drupal/cas_attributes": "^2.0@beta",
        "drupal/checklistapi": "^2.0",
        "drupal/coder": "8.3.13",
        "drupal/coffee": "1.2.0",
        "drupal/config_update": "^1.7",
        "drupal/contact_ajax": "^1.5",
        "drupal/contact_block": "^1.5",
        "drupal/content_moderation_notifications": "3.3.0",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/crop": "2.1",
        "drupal/csv_importer": "1.11.0",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "3.7.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/diff": "1.0",
        "drupal/domain": "1.0.0-beta5",
        "drupal/editor_advanced_link": "1.9.0",
        "drupal/embed": "1.4",
        "drupal/emergency_alerts": "2.x-dev",
        "drupal/entity_browser": "2.6.0",
        "drupal/entity_clone": "1.0.0-beta6",
        "drupal/entity_reference_revisions": "1.9.0",
        "drupal/entity_usage": "2.0-beta3",
        "drupal/entityqueue": "^1.2",
        "drupal/externalauth": "1.4.0",
        "drupal/facets": "^1.8",
        "drupal/fakeobjects": "1.1.0",
        "drupal/features": "^3.12",
        "drupal/feeds": "3.0.0-alpha10",
        "drupal/feeds_ex": "1.0-alpha4",
        "drupal/feeds_tamper": "2.0-beta2",
        "drupal/field_group": "3.1.0",
        "drupal/flood_unblock": "3.1.0",
        "drupal/google_analytics": "2.5.0",
        "drupal/google_cse": "3.4.0",
        "drupal/image_url_formatter": "^1.0",
        "drupal/image_widget_crop": "2.3+5-dev",
        "drupal/imageapi_optimize": "4.0.0-beta1",
        "drupal/imageapi_optimize_resmushit": "2.0.0-beta1",
        "drupal/imce": "2.4.0",
        "drupal/libraries": "3.0.0-beta1",
        "drupal/link_attributes": "1.11.0",
        "drupal/linkit": "5.0.0-beta12",
        "drupal/ludwig": "1.3.0",
        "drupal/mailsystem": "4.3",
        "drupal/masquerade": "2.0.0-beta4",
        "drupal/menu_block": "1.7.0",
        "drupal/menu_breadcrumb": "1.15.0",
        "drupal/metatag": "1.16.0",
        "drupal/pantheon_advanced_page_cache": "1.2.0",
        "drupal/paragraphs": "1.12",
        "drupal/paragraphs_edit": "2.0-alpha8",
        "drupal/redis": "1.5.0",
        "drupal/scanner": "1.0.0-rc5",
        "drupal/search_api": "1.x-dev#fb4e33d",
        "drupal/search_api_autocomplete": "1.4",
        "drupal/search_api_solr": "4.1.12",
        "drupal/sendgrid_integration": "2.0.0-alpha5",
        "drupal/sftp_client": "1.0.x-dev@dev",
        "drupal/shs": "1.0.0-alpha5",
        "drupal/simple_sitemap": "3.10.0",
        "drupal/simplenews": "^2.0@beta",
        "drupal/superfish": "1.4",
        "drupal/tac_lite": "1.5",
        "drupal/tamper": "1.0-alpha2",
        "drupal/taxonomy_manager": "2.0.4",
        "drupal/token": "1.9.0",
        "drupal/token_filter": "1.2",
        "drupal/twig_tweak": "2.9.0",
        "drupal/typed_data": "1.0-alpha5",
        "drupal/views_autocomplete_filters": "1.3",
        "drupal/views_bootstrap": "4.3",
        "drupal/views_bulk_edit": "2.6.0",
        "drupal/views_bulk_operations": "3.11.0",
        "drupal/views_entity_form_field": "1.0.0-beta9",
        "drupal/views_fieldsets": "3.4.0",
        "drupal/views_show_more": "1.0",
        "drupal/yoast_seo": "1.7.0",
        "drupal/youtube": "1.2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}

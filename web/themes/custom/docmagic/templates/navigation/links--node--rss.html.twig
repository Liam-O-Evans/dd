{# rss links #}
{# @see docmagic_theme_suggestions_links_alter() #}
{%- if links -%}
    <p>
        {%- for key, item in links -%}
            {%- if item.link -%}
                {{ item.link }}
            {%- elseif item.text_attributes -%}
                <span{{ item.text_attributes }}>{{ item.text }}</span>
            {%- else -%}
                {{ item.text }}
            {%- endif -%}
        {%- endfor -%}
    </p>
{%- endif -%}
